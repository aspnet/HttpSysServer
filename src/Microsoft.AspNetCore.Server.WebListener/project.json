{
  "version": "1.0.2",
  "description": "ASP.NET Core HTTP server for Windows.",
  "packOptions": {
    "tags": [
      "aspnetcore",
      "weblistener"
    ]
  },
  "dependencies": {
    "Microsoft.AspNetCore.Hosting": "1.0.2",
    "Microsoft.Net.Http.Headers": "1.0.2",
    "Microsoft.Net.Http.Server": "1.0.2"
  },
  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "frameworks": {
    "net451": {},
    "netstandard1.3": {
      "dependencies": {
        "System.Security.Claims": "4.0.1"
      }
    }
  }
}